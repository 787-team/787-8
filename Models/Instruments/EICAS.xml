<!-- TAT digits -->

<!-- Positive TAT Value -->

	<animation>
		<type>select</type>
		<object-name>TAT.1</object-name>
		<object-name>TAT.10</object-name>
		<condition>
			<greater-than-equals>
				<property>controls/ice/tat</property>
				<value>0</value>
			</greater-than-equals>
		</condition>
	</animation>

<!-- Negative TAT Value -->

	<animation>
		<type>select</type>
		<object-name>TAT.-</object-name>
		<object-name>TAT.-1</object-name>
		<object-name>TAT.-10</object-name>
		<condition>
			<less-than>
				<property>controls/ice/tat</property>
				<value>0</value>
			</less-than>
		</condition>
	</animation>


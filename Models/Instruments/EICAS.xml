<?xml version="1.0"?>

<PropertyList>

	<path>eicas.ac</path>

<!-- Come on only when there's atleast 9 volts of power -->

	<animation>
		<type>select</type>
		<object-name>eicas</object-name>
		<condition>
			<greater-than>
				<property>systems/electrical/outputs/efis</property>
				<value>9</value>
			</greater-than>
		</condition>
	</animation>


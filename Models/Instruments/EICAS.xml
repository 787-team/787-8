<!-- N1, N2 and EGT Rotate Animations -->

<animation>
	<type>rotate</type>
   	<object-name>n1.l</object-name>
   	<property>engines/engine/n1</property>
   	<interpolation>
        <entry>
       		<ind>0.0</ind><dep>0.0</dep>
     	</entry>
     	<entry>
       		<ind>100</ind><dep>225</dep>
     	</entry>
   	</interpolation>
   	<center>
    	<x-m>0</x-m>
     	<y-m>-0.0414</y-m>
     	<z-m>0.1128</z-m>
